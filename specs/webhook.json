        { "name": "avatar", "type":{ "base":"char", "dec":"*" }, "comment":"@todo fixed size limit"},
        { "name": "token", "type":{ "base":"char", "dec":"*" }, "comment":"@todo fixed size limit"},
        { "name": "application_id", "type":{ "base":"char", "dec":"*", "converter":"snowflake" } },
